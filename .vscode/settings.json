{
  "yaml.schemas": {
    "https://json.schemastore.org/github-workflow.json": ".github/workflows/*.yml"
  },
  "yaml.schemaStore.enable": true,
  "yaml.validate": true,
  "yaml.customTags": [
    "!And",
    "!If",
    "!Not",
    "!Equals",
    "!Or",
    "!FindInMap",
    "!Base64",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!Select",
    "!Split",
    "!Join"
  ],
  "files.associations": {
    "*.yml": "yaml",
    ".luacheckrc": "lua"
  },
  "Lua.diagnostics.globals": [
    "Citizen",
    "CreateThread",
    "Wait",
    "SetTimeout",
    "GetResourceState",
    "GetCurrentResourceName",
    "LoadResourceFile",
    "SaveResourceFile",
    "IsDuplicityVersion",
    "RegisterCommand",
    "RegisterNetEvent",
    "AddEventHandler",
    "RemoveEventHandler",
    "TriggerEvent",
    "TriggerServerEvent",
    "TriggerClientEvent",
    "RegisterKeyMapping",
    "PlayerPedId",
    "GetPlayerPed",
    "PlayerId",
    "GetPlayerName",
    "GetPlayerIdentifiers",
    "source",
    "GetEntityCoords",
    "GetEntityHeading",
    "GetEntityModel",
    "IsPedInAnyVehicle",
    "GetPedInVehicleSeat",
    "GetVehiclePedIsIn",
    "GetVehicleClass",
    "GetDisplayNameFromVehicleModel",
    "SetVehicleLights",
    "SetVehicleIndicatorLights",
    "SetNotificationTextEntry",
    "AddTextComponentString",
    "DrawNotification",
    "PlaySoundFrontend",
    "CreateAudioSubmix",
    "SetAudioSubmixEffectRadioFx",
    "SetAudioSubmixEffectParamInt",
    "SetAudioSubmixEffectParamFloat",
    "AddAudioSubmixOutput",
    "MumbleSetAudioInputIntent",
    "MumbleSetSubmixForServerId",
    "exports",
    "ESX",
    "QBCore",
    "lib",
    "MySQL",
    "fx_version",
    "game",
    "lua54",
    "author",
    "description",
    "version",
    "dependency",
    "shared_scripts",
    "client_scripts",
    "server_scripts",
    "Config",
    "GetPlayerJob",
    "RegisterFrameworkEvents",
    "IsPlayerInAllowedVehicle",
    "IsPlayerInAllowedJob",
    "CanUseCarMegaphone",
    "ShowNotification",
    "ActivateMegaphone",
    "DeactivateMegaphone",
    "ActivateVisualEffects",
    "DeactivateVisualEffects",
    "ApplyMegaphoneSubmix",
    "RemoveMegaphoneSubmix",
    "LogMegaphoneUsage"
  ],
  "Lua.diagnostics.disable": [
    "lowercase-global"
  ],
  "Lua.workspace.checkThirdParty": false,
  "markdownlint.config": {
    "MD033": false,
    "MD036": false,
    "MD040": false
  },
  "github-actions.workflows.pinned.workflows": [
    ".github/workflows/lua-lint.yml",
    ".github/workflows/release.yml"
  ],
    "problems.decorations.enabled": true,
  "yaml.validate": true,
  "github-actions.diagnostics.disabled": [
    "action-not-found",
    "context-access-invalid"
  ],
  "github-actions.offline-mode": false  
}
